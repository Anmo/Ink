---
---
[
  {"ts":"{{site.time | date: '%d-%m-%Y'}}"},
  [{% for page in site.pages %}
    {
     "url":"{{ page.url | remove:'index.html' }}",
     "tags":"{{ page.tags }}",
     "title":"{{ page.title }}",
     "content":"{{ page.content | strip_html | strip_newlines | remove: "\" | remove: '"' | remove: "  " | remove: "   " | remove: "    " | remove: "     " | remove: "      " | remove: "       " | remove: "        " }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}]
]


