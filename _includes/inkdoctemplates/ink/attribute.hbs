<section id="{{{hash}}}" class="box box-attribute">
    <h1 class="fw-300">
    {{#ifvalue parent.kind value="class"}}
        .{{name}}
    {{else}}
        {{name}}
    {{/ifvalue}}
    <i>attribute</i></h1>
    {{#if deprecated}}
        <span class="tag">deprecated</span>
    {{/if}}
    {{#if type}}
        <pre>{{type}}</pre>
    {{/if}}
    {{#if text}}
        <p class="text">{{{stripP text}}}</p>
    {{/if}}
    {{#if description}}
        <span class="description">{{{description}}}</span>
    {{/if}}
</section>
