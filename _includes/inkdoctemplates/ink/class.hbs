    {{#if functions.length}}
    <table class="props bottom-space">
        
        {{#if isNamespace}}
        <caption>FUNCTIONS</caption>
        <thead>
            <tr>
                <th>Function name</th>
                <th>Description</th>
            </tr>
        </thead>

        {{else}}
        <caption>METHODS</caption>
        <thead>
            <tr>
                <th>Method name</th>
                <th>Description</th>
            </tr>
        </thead>
        {{/if}}
        
        <tbody>
        {{#each functions}}
            <tr>
                <td>
                    <a href="#{{{hash}}}">
                        {{#if isConstructor}}
                            {{parent.nam0e}}
                        {{else}}
                            .{{name}}
                        {{/if}}
                        
                    </a>
                </td>
                <td>
                    {{#if deprecated}}
                        <span class="tag">deprecated</span>
                    {{/if}}

                    {{#if text}}{{{text}}}{{/if}}

                    {{#if type}}
                        <pre>{{type}}</pre>
                    {{/if}}
                    {{#if description}}{{{description}}}{{/if}}
                </td>
            </tr>
        {{/each}}
        </tbody>
    </table>
    {{/if}}

    {{#if attributes.length}}
    <table class="props">
        <caption>PROPERTIES</caption>
        <thead>
            <tr>
                <th>Property name</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
            {{#each attributes}}
            <tr>
                <td><a href="#{{{hash}}}">.{{name}}</a></td>
                <td>
                    {{#if deprecated}}
                        <span class="tag">deprecated</span>
                    {{/if}}

                    {{#if text}}
                        {{{text}}}
                    {{/if}}

                    {{#if type}}
                        <code>{{type}}</code>
                    {{/if}}
                    {{#if description}}
                        <span class="description">{{{description}}}</span>
                    {{/if}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    {{/if}}
</section>


{{#if text}}
    <div class="text">{{{text}}}</div>
{{/if}}

{{!--{{{> examples}}}--}}
{{> samples}}


{{#each functions}}
    {{> function}}
{{/each}}

{{#each attributes}}
    {{> attribute}}
{{/each}}